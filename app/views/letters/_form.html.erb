<%= semantic_form_for(letter) do |f| %>
  <%= f.inputs do %>
    <%= f.input :recieved %>
    <%= f.input :published %>
    <%= f.input :is_draft %>
    <%= f.input :content %>
    <%= f.input :preview %>
    <%= f.file_field :scans, multiple: true %>
  <% end %>
  <%= f.actions do %>
    <%= f.action :submit %>
  <% end %>
<% end %>

<%= semantic_form_for @event do |f| %>
  <%= f.inputs do %>
    <%= f.input :title %>
    <%= f.input :start %>
    <%= f.input :finish %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :street1 %>
    <%= f.input :street2 %>
    <%= f.input :city %>
    <%= f.input :postcode %>
    <%= f.input :eventbrite %>
    <%= f.input :facebook %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
